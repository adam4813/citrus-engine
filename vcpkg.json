{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "citrus-engine",
  "version": "0.0.1",
  "dependencies": [
    "angelscript",
    "bullet3",
    "flecs",
    {
      "name": "glad",
      "platform": "!(emscripten)"
    },
    "glfw3",
    "glm",
    "gtest",
    {
      "name": "imgui",
      "features": [
        "glfw-binding",
        "opengl3-binding"
      ]
    },
    "joltphysics",
    "lua",
    "nlohmann-json",
    "spdlog",
    "stb"
  ],
  "supports": "windows | linux | osx | emscripten",
  "builtin-baseline": "dd3097e305afa53f7b4312371f62058d2e665320",
  "overrides": [
    {
      "name": "spdlog",
      "version": "1.11.0"
    },
    {
      "name": "fmt",
      "version": "9.0.0"
    }
  ]
}
